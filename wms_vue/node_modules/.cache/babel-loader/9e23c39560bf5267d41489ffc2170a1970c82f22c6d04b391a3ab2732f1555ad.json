n options\"\r\n          :key=\"item.value\"\r\n          :label=\"item.label\"\r\n          :value=\"item.value\">\r\n        </el-option>\r\n      </el-select>\r\n\r\n      <el-input v-model=\"input\" placeholder=\"请输入用户名\" style=\"width:20%; margin-left: 10px\"></el-input> \r\n\r\n      <el-button type=\"primary\" style=\"margin-left: 40px\"><i class=\"el-icon-search\"></i> 查询</el-button>\r\n  </div>\r\n  \r\n\r\n<!-- 表单区域 -->\r\n  <el-table :data=\"tableData\" stripe border style=\"width: 100%\" \r\n    :cell-style=\"{textAlign:'center'}\" :header-cell-style=\"{textAlign:'center'}\">\r\n    <el-table-column prop=\"userid\" label=\"用户id\"> </el-table-column>\r\n    <el-table-column prop=\"username\" label=\"用户名称\" > </el-table-column>\r\n    <el-table-column prop=\"roleid\" label=\"角色id\" > </el-table-column>\r\n    <el-table-column v-model=\"formattedTime\" prop=\"time\" label=\"入职时间\" > </el-table-column>\r\n    <el-table-column fixed=\"right\" label=\"操作\" width=\"180\" >\r\n      <template v-slot=\"scope\">\r\n        <el-button @click=\"handleClick(scope.row)\" type=\"text\" size=\"medium\">查看</el-button>\r\n        <el-button type=\"text\" size=\"medium\">编辑</el-button>\r\n      </template>\r\n    </el-table-column>\r\n  </el-table>\r\n\r\n\r\n<!-- 分页组件 -->\r\n  <div class=\"block\" style=\"margin-top: 30px; text-align:right\">\r\n  <el-pagination\r\n    layout=\"prev, pager, next\"\r\n    :total=\"1000\">\r\n  </el-pagination>\r\n  </div>\r\n\r\n </div>\r\n</template>\r\n\r\n<script>\r\nimport request from '@/request/request'\r\nimport dayjs from 'dayjs';\r\nexport default {\r\n  created(){\r\n    this.load();\r\n  },\r\n    data() {\r\n      return {\r\n        input:'',\r\n        value1: '',\r\n        tableData: [],\r\n        time: new Date(),\r\n\r\n        options: [{\r\n          value: '选项1',\r\n          label: '仓库1'\r\n        }, {\r\n          value: '选项2',\r\n          label: '仓库2'\r\n        }, {\r\n          value: '选项3',\r\n          label: '仓库3'\r\n        }, {\r\n          value: '选项4',\r\n          label: '仓库4'\r\n        }, {\r\n          value: '选项5',\r\n          label: '仓库5'\r\n        }],\r\n        value: ''\r\n      }\r\n    },\r\n    methods: {\r\n      load(){\r\n        request.get('/user/allUserData').then(res => {\r\n          if(res.code == 0){\r\n            this.tableData = res.data; \r\n          }\r\n          else{\r\n            this.$notify.error(res.msg);\r\n          }\r\n    \r\n        })\r\n      },\r\n\r\n    },\r\n    computed: {\r\n    // 格式化时间\r\n    formattedTime() {\r\n      return this.user.time \r\n        ? dayjs(this.user.time).format('YYYY-MM-DD') \r\n        : '';\r\n    }\r\n  }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n.title{\r\n  text-align:left;\r\n}\r\n \r\n</style>\r\n"],"mappings":"AAyDA,OAAAA,OAAA;AACA,OAAAC,KAAA;AACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,KAAA;IACA;MACAC,KAAA;MACAC,MAAA;MACAC,SAAA;MACAC,IAAA,MAAAC,IAAA;MAEAC,OAAA;QACAC,KAAA;QACAC,KAAA;MACA;QACAD,KAAA;QACAC,KAAA;MACA;QACAD,KAAA;QACAC,KAAA;MACA;QACAD,KAAA;QACAC,KAAA;MACA;QACAD,KAAA;QACAC,KAAA;MACA;MACAD,KAAA;IACA;EACA;EACAE,OAAA;IACAV,KAAA;MACAH,OAAA,CAAAc,GAAA,sBAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAV,SAAA,GAAAS,GAAA,CAAAZ,IAAA;QACA,OACA;UACA,KAAAc,OAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MAEA;IACA;EAEA;EACAC,QAAA;IACA;IACAC,cAAA;MACA,YAAAC,IAAA,CAAAf,IAAA,GACAP,KAAA,MAAAsB,IAAA,CAAAf,IAAA,EAAAgB,MAAA,iBACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}