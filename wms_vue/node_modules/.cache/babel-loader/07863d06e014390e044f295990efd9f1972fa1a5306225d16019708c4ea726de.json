{"ast":null,"code":"import request from '@/request/request';\nexport default {\n  name: 'app',\n  data() {\n    return {\n      applications: [],\n      completeds: [],\n      ruleForm: {\n        pname: '',\n        type: '',\n        deposityOld: '',\n        deposityNew: '',\n        quantity: '',\n        price: '',\n        state: '待审核',\n        applyId: '',\n        applyTime: '',\n        applyRemark: '',\n        reviewId: ''\n      },\n      rules: {\n        pname: [{\n          required: true,\n          message: '请输入产品名称',\n          trigger: 'blur'\n        }],\n        quantity: [{\n          required: true,\n          message: '请输入产品数量',\n          trigger: 'blur'\n        }, {\n          validator: (rule, value, callback) => {\n            if (typeof value !== 'number' || isNaN(value)) {\n              callback(new Error('必须输入数字'));\n            } else if (value < 0) {\n              callback(new Error('数量不能为负数'));\n            } else {\n              callback();\n            }\n          },\n          trigger: ['blur']\n        }],\n        price: [{\n          required: true,\n          message: '请输入产品价格',\n          trigger: 'blur'\n        }, {\n          validator: (rule, value, callback) => {\n            if (typeof value !== 'number' || isNaN(value)) {\n              callback(new Error('必须输入数字'));\n            } else if (value < 0) {\n              callback(new Error('价格不能为负数'));\n            } else {\n              callback();\n            }\n          },\n          trigger: ['blur']\n        }],\n        deposityNew: [{\n          required: true,\n          message: '请选择仓库',\n          trigger: 'change'\n        }],\n        type: [{\n          required: true,\n          message: '请选择操作类型',\n          trigger: 'change'\n        }],\n        reviewId: [{\n          required: true,\n          message: '请选择审核人',\n          trigger: 'change'\n        }]\n      }\n    };\n  },\n  mounted() {\n    this.loadpendings();\n    this.loadcompleteds();\n  },\n  methods: {\n    loadpendings() {\n      request.get('/recordIn/myPendingData').then(res => {\n        if (res.code == 0) {\n          console.log(res.data);\n          this.applications = res.data;\n        }\n      });\n    },\n    loadcompleteds() {\n      request.get('/recordIn/myCompletedData').then(res => {\n        if (res.code == 0) {\n          this.completeds = res.data;\n        }\n      });\n    },\n    pendingDetail(application) {\n      //编辑\n      console.log(\"当前卡片的数据：\", application);\n      this.ruleForm = application;\n      this.editFormVisible = true;\n    },\n    apply() {\n      this.$refs.ruleForm.validate(valid => {\n        this.ruleForm.deposityOld = this.user.deposity;\n        this.ruleForm.applyId = this.user.userid;\n        console.log(this.ruleForm);\n        if (valid) {\n          this.$request.post('/recordIn/update', this.ruleForm).then(res => {\n            if (res.code === 0) {\n              this.$message.success(\"修改成功\");\n              this.active = 1;\n            } else {\n              this.$message.error(res.msg);\n            }\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","name","data","applications","completeds","ruleForm","pname","type","deposityOld","deposityNew","quantity","price","state","applyId","applyTime","applyRemark","reviewId","rules","required","message","trigger","validator","rule","value","callback","isNaN","Error","mounted","loadpendings","loadcompleteds","methods","get","then","res","code","console","log","pendingDetail","application","editFormVisible","apply","$refs","validate","valid","user","deposity","userid","$request","post","$message","success","active","error","msg"],"sources":["src/components/application/myapplication.vue"],"sourcesContent":["<template>\r\n <div >\r\n    <div style=\"margin-top:20px\">\r\n        <div style=\"margin-bottom:20px ;font-size:20px;text-align:left\">已提交申请（{{ applications.length }}）</div>\r\n        <el-row :gutter=\"20\">\r\n            <el-col v-for=\"application in applications\" :key=\"application.recordId\" :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"6\">\r\n                <el-card class=\"application-card\" shadow=\"hover\" style=\"text-align:left\">\r\n                    <!-- 卡片头部 -->\r\n                    <div slot=\"header\" class=\"card-header\">\r\n                    <span>{{ application.applyId }}的申请</span>\r\n                    <el-button type=\"warning\" size=\"mini\" effect=\"dark\" style=\"float:right\" @click=\"pendingDetail(application)\">待审核</el-button>\r\n                    </div>\r\n                    <!-- 申请内容 -->\r\n                    <div>\r\n                        <div>申请类型：{{ application.type }}</div>\r\n                        <div>变动产品：{{application.pname}}</div>\r\n                        <div>产品去向：{{application.deposityOld}}————{{application.deposityNew}}</div>\r\n                        <div>申请时间：{{ application.applyTime }}</div>\r\n                        <div>审核人：{{ application.reviewId }}</div>\r\n                    </div>\r\n                </el-card>\r\n            </el-col>\r\n        </el-row>\r\n    </div>\r\n\r\n\r\n    <div style=\"margin-top:20px\">\r\n        <div style=\"margin-bottom:20px ;font-size:20px;text-align:left\">已通过申请（{{ completeds.length }}）</div>\r\n        <el-row :gutter=\"20\">\r\n            <el-col v-for=\"completed in completeds\" :key=\"completed.recordId\" :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"6\">\r\n                <el-card class=\"completed-card\" shadow=\"hover\" style=\"text-align:left\">\r\n                    <!-- 卡片头部 -->\r\n                    <div slot=\"header\" class=\"card-header\">\r\n                    <span>{{ completed.applyId }}的申请</span>\r\n                    <el-button type=\"success\" size=\"mini\" effect=\"dark\" style=\"float:right\" @click=\"completedDetail\">已通过</el-button>\r\n                    </div>\r\n                    <!-- 申请内容 -->\r\n                    <div>\r\n       