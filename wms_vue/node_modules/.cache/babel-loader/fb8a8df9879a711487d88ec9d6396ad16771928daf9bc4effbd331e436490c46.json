{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport request from '@/request/request';\nimport dayjs from 'dayjs';\nexport default {\n  created() {\n    this.load();\n  },\n  data() {\n    return {\n      value1: '',\n      tableData: [],\n      pageNum: 1,\n      //当前页码\n      pageSize: 5,\n      //每页个数\n      pname: '',\n      recordId: '',\n      total: 0,\n      deposity: '',\n      record_ids: [],\n      user: JSON.parse(localStorage.getItem('user') || '{}'),\n      deposity_in: '',\n      deposity_out: '',\n      date: ''\n    };\n  },\n  methods: {\n    reset() {\n      this.pname = '';\n      this.deposity_in = '';\n      this.deposity_out = '';\n      this.date = '';\n      this.load(this.pageNum);\n    },\n    handleImport(res) {\n      if (res.code === 0) {\n        this.$message.success(\"导入成功\");\n        this.load(1);\n      } else {\n        this.$message.error(res.msg);\n      }\n    },\n    exportData() {\n      //批量导出\n      console.log(this.record_ids.length);\n      if (!this.record_ids.length) {\n        //没有选择行的时候，全部导出，或者根据搜索条件查询到的数据全部导出\n        window.open('http://localhost:9000/recordIn/export?token=' + this.user.token + \"&pname=\" + this.pname + \"&deposity=\" + this.deposity);\n      } else {\n        let record_idsStr = this.record_ids.join(',');\n        window.open('http://localhost:9000/recordIn/export?token=' + this.user.token + \"&record_ids=\" + record_idsStr); //传了选中的行\n      }\n    },\n    handleSelectionChange(rows) {\n      console.log(\"原始 rows 数据:\", rows);\n      this.record_ids = rows.map(v => v.recordId); //把对象数组变成数字数组,v.recordId与前端获取的row的字段名一致，刚开始写recoord_id一直不成功\n      console.log(this.record_ids[0]);\n    },\n    load(pageNum) {\n      //分页查询\n      if (pageNum) {\n        this.pageNum = pageNum;\n      }\n      request.get('/recordIn/selectByPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          pname: this.pname,\n          deposityIn: this.deposity_in,\n          deposityOut: this.deposity_out,\n          date: this.date\n        }\n      }).then(res => {\n        if (res.code == 0) {\n          this.tableData = res.data.records;\n          this.total = res.data.total;\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    formatTime(row, column, cellValue) {\n      if (!cellValue) return '暂无时间'; // 处理空值\n      return dayjs(cellValue).format('YYYY-MM-DD');\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["request","dayjs","created","load","data","value1","tableData","pageNum","pageSize","pname","recordId","total","deposity","record_ids","user","JSON","parse","localStorage","getItem","deposity_in","deposity_out","date","methods","reset","handleImport","res","code","$message","success","error","msg","exportData","console","log","length","window","open","token","record_idsStr","join","handleSelectionChange","rows","map","v","get","params","deposityIn","deposityOut","then","records","$notify","formatTime","row","column","cellValue","format","handleCurrentChange"],"sources":["src/components/manager_in/search_in.vue"],"sourcesContent":["<template>\r\n <div>\r\n  \r\n  <div style=\"margin-top:20px; margin-bottom: 30px; display:flex;\">\r\n<!-- 查询组件 -->\r\n      <!-- <el-select style=\"margin-left: 10px;\" v-model=\"value\" filterable placeholder=\"请选择来源仓库\">\r\n        <el-option\r\n          v-for=\"item in options\"\r\n          :key=\"item.value\"\r\n          :label=\"item.label\"\r\n          :value=\"item.value\">\r\n        </el-option>\r\n      </el-select> -->\r\n      <!-- <el-input v-model=\"deposity\" placeholder=\"请输入来源仓库\" style=\"width:20%; margin-left: 10px\"></el-input> -->\r\n      <el-select v-model=\"deposityIn\" placeholder=\"请选择进货仓库\" style=\"width:20%; margin-left: 10px\">\r\n        <el-option v-for=\"item in ['仓库1','仓库2','仓库3','仓库4','仓库5']\" :key=\"item\" :label=\"item\" :value=\"item\"></el-option>\r\n      </el-select>\r\n      <el-select v-model=\"deposityOut\" placeholder=\"请选择出货仓库\" style=\"width:20%; margin-left: 10px\">\r\n        <el-option v-for=\"item in ['仓库1','仓库2','仓库3','仓库4','仓库5']\" :key=\"item\" :label=\"item\" :value=\"item\"></el-option>\r\n      </el-select>\r\n      \r\n      <div class=\"block\" style=\"margin-left: 10px\">\r\n        <el-date-picker\r\n          v-model=\"value1\"\r\n          type=\"date\"\r\n          placeholder=\"选择日期\">\r\n        </el-date-picker>\r\n      </div>\r\n\r\n      <el-input v-model=\"pname\" placeholder=\"请输入产品名称\" style=\"width:20%; margin-left: 10px\"></el-input> \r\n\r\n      <el-button type=\"primary\" style=\"margin-left: 40px\" @click=\"load(1)\"><i class=\"el-icon-search\"></i> 查询</el-button>\r\n      <el-button type=\"primary\" style=\"margin-left: 10px\" @click=\"reset\"><i class=\"el-icon-refresh-right\"></i> 重置</el-button>\r\n  </div>\r\n\r\n\r\n  <div style=\"margin-top: 20px;margin-bottom: 10px\">\r\n    <!-- <el-button type=\"primary\" plain @click=\"handleAdd\">新增</el-button>\r\n    <el-button type=\"danger\" plain @click=\"delBatch\">批量删除</el-button> -->\r\n    <el-button type=\"primary\" plain @click=\"exportData\">批量导出</el-button>\r\n    <el-upload action=\"http://localhost:9000/recordIn/import\" :headers=\"{token:user.token}\" :on-success=\"handleImport\" style=\"display:inline-block ;margin-left: 10px\" :show-file-list=\"false\">\r\n      <el-button type=\"primary\" plain>批量导入</el-button>\r\n    </el-upload>\r\n  </div>\r\n  \r\n\r\n<!-- 表单区域 -->\r\n  <el-table :data=\"tableData\" stripe border style=\"width: 100%\" \r\n    :cell-style=\"{textAlign:'center'}\" :header-cell-style=\"{textAlign:'center',backgroundColor:'aliceblue',color:'#666'}\"\r\n    @selection-change=\"handleSelectionChange\">\r\n    <!-- 多选 -->\r\n    <el-table-column type=\"selection\" width=\"55\" align=\"center\"> </el-table-column>\r\n    <el-table-column prop=\"recordId\" label=\"记录编号\" width=\"90\"> </el-table-column>\r\n    <el-table-column prop=\"pname\" label=\"产品名称\" > </el-table-column>\r\n    <el-table-column prop=\"type\" label=\"记录类型\" > </el-table-column>\r\n    <el-table-column prop=\"deposity_in\" label=\"进货仓库\" width=\"90\"> </el-table-column>\r\n    <el-table-column prop=\"deposity_out\" label=\"出货仓库\" width=\"90\"> </el-table-column>\r\n    <el-table-column prop=\"quantity\" label=\"数量\" width=\"90\"> </el-table-column>\r\n    <el-table-column prop=\"price\" label=\"价格\" width=\"90\"> </el-table-column>\r\n    <el-table-column prop=\"state\" label=\"当前状态\" width=\"90\"> </el-table-column>\r\n    <el-table-column prop=\"applyId\" label=\"申请人\" width=\"90\"> </el-table-column>\r\n    <el-table-column prop=\"applyTime\" label=\"申请时间\" width=\"180\" :formatter=\"formatTime\"> </el-table-column>\r\n    <el-table-column prop=\"reviewId\" label=\"审核人\" width=\"90\"> </el-table-column>\r\n    <el-table-column prop=\"reviewTime\" label=\"审核时间\" width=\"180\" :formatter=\"formatTime\"> </el-table-column>\r\n    <!-- <el-table-column fixed=\"right\" label=\"操作\" >\r\n      <template slot-scope=\"scope\">\r\n        <el-button  type=\"text\" size=\"medium\" @click=\"handleEdit(scope.row)\">编辑</el-button>\r\n        <el-button type=\"text\" size=\"medium\" @click=\"del(scope.row.userid)\">删除</el-button>\r\n      </template>\r\n    </el-table-column> -->\r\n  </el-table>\r\n\r\n\r\n<!-- 分页组件 -->\r\n  <div style=\"margin:10px\">\r\n    <el-pagination\r\n      @current-change=\"handleCurrentChange\"\r\n      :current-page=\"pageNum\"\r\n      :page-sizes=\"[5, 10, 15, 20]\"\r\n      :page-size=\"pageSize\"\r\n      layout=\"total, prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n\r\n\r\n\r\n\r\n\r\n\r\n </div>\r\n</template>\r\n\r\n<script>\r\nimport request from '@/request/request'\r\nimport dayjs from 'dayjs'\r\n export default {\r\n  created(){\r\n    this.load();\r\n  },\r\n    data() {\r\n      return {\r\n        value1: '',\r\n        tableData: [],\r\n        pageNum: 1,//当前页码\r\n        pageSize: 5,//每页个数\r\n        pname: '',\r\n        recordId: '',\r\n        total: 0,\r\n        deposity: '',\r\n        record_ids: [],\r\n        user:JSON.parse(localStorage.getItem('user') || '{}'),\r\n        deposity_in: '',\r\n        deposity_out: '',\r\n        date: '',\r\n\r\n      }\r\n    },\r\n    methods: {\r\n      reset(){\r\n        this.pname=''\r\n        this.deposity_in=''\r\n        this.deposity_out=''\r\n        this.date=''\r\n        this.load(this.pageNum)\r\n      },\r\n      handleImport(res){\r\n        if(res.code===0){\r\n          this.$message.success(\"导入成功\");\r\n          this.load(1)\r\n        }else{\r\n          this.$message.error(res.msg);\r\n        }\r\n      },\r\n      exportData(){//批量导出\r\n      console.log(this.record_ids.length)\r\n      if(!this.record_ids.length){//没有选择行的时候，全部导出，或者根据搜索条件查询到的数据全部导出\r\n        window.open('http://localhost:9000/recordIn/export?token='+ this.user.token + \"&pname=\"+this.pname + \"&deposity=\"+this.deposity )\r\n      }else{\r\n        let record_idsStr = this.record_ids.join(',')\r\n        window.open('http://localhost:9000/recordIn/export?token='+ this.user.token + \"&record_ids=\"+record_idsStr)//传了选中的行\r\n      }\r\n      },\r\n      handleSelectionChange(rows){   \r\n        console.log(\"原始 rows 数据:\", rows);\r\n        this.record_ids=rows.map(v=>v.recordId)//把对象数组变成数字数组,v.recordId与前端获取的row的字段名一致，刚开始写recoord_id一直不成功\r\n        console.log(this.record_ids[0])\r\n      },\r\n      load(pageNum){//分页查询\r\n      if(pageNum)\r\n      {\r\n        this.pageNum=pageNum\r\n      }\r\n        request.get('/recordIn/selectByPage', {\r\n          params: {\r\n            pageNum: this.pageNum,\r\n            pageSize: this.pageSize,\r\n            pname: this.pname,\r\n            deposityIn:this.deposity_in,\r\n            deposityOut:this.deposity_out,\r\n            date:this.date\r\n          }\r\n        }).then(res => {\r\n          if(res.code == 0){\r\n            this.tableData = res.data.records\r\n            this.total=res.data.total\r\n          }\r\n          else{\r\n            this.$notify.error(res.msg);\r\n          }\r\n    \r\n        })\r\n      },\r\n      formatTime(row, column, cellValue) {\r\n      if (!cellValue) return '暂无时间'; // 处理空值\r\n      return dayjs(cellValue).format('YYYY-MM-DD');\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      this.pageNum=pageNum\r\n      this.load(pageNum)\r\n    },\r\n\r\n\r\n\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n.title{\r\n  text-align:left;\r\n}\r\n \r\n</style>\r\n"],"mappings":";;AA8FA,OAAAA,OAAA;AACA,OAAAC,KAAA;AACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,KAAA;IACA;MACAC,MAAA;MACAC,SAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,QAAA;MACAC,KAAA;MACAC,QAAA;MACAC,UAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,WAAA;MACAC,YAAA;MACAC,IAAA;IAEA;EACA;EACAC,OAAA;IACAC,MAAA;MACA,KAAAd,KAAA;MACA,KAAAU,WAAA;MACA,KAAAC,YAAA;MACA,KAAAC,IAAA;MACA,KAAAlB,IAAA,MAAAI,OAAA;IACA;IACAiB,aAAAC,GAAA;MACA,IAAAA,GAAA,CAAAC,IAAA;QACA,KAAAC,QAAA,CAAAC,OAAA;QACA,KAAAzB,IAAA;MACA;QACA,KAAAwB,QAAA,CAAAE,KAAA,CAAAJ,GAAA,CAAAK,GAAA;MACA;IACA;IACAC,WAAA;MAAA;MACAC,OAAA,CAAAC,GAAA,MAAApB,UAAA,CAAAqB,MAAA;MACA,UAAArB,UAAA,CAAAqB,MAAA;QAAA;QACAC,MAAA,CAAAC,IAAA,uDAAAtB,IAAA,CAAAuB,KAAA,oBAAA5B,KAAA,uBAAAG,QAAA;MACA;QACA,IAAA0B,aAAA,QAAAzB,UAAA,CAAA0B,IAAA;QACAJ,MAAA,CAAAC,IAAA,uDAAAtB,IAAA,CAAAuB,KAAA,oBAAAC,aAAA;MACA;IACA;IACAE,sBAAAC,IAAA;MACAT,OAAA,CAAAC,GAAA,gBAAAQ,IAAA;MACA,KAAA5B,UAAA,GAAA4B,IAAA,CAAAC,GAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAjC,QAAA;MACAsB,OAAA,CAAAC,GAAA,MAAApB,UAAA;IACA;IACAV,KAAAI,OAAA;MAAA;MACA,IAAAA,OAAA,EACA;QACA,KAAAA,OAAA,GAAAA,OAAA;MACA;MACAP,OAAA,CAAA4C,GAAA;QACAC,MAAA;UACAtC,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAC,KAAA,OAAAA,KAAA;UACAqC,UAAA,OAAA3B,WAAA;UACA4B,WAAA,OAAA3B,YAAA;UACA