blur' }\r\n          ],\r\n        },\r\n\r\n\r\n\r\n       }\r\n    },\r\n    components: {\r\n\r\n    },\r\n    methods: { \r\n      update(){\r\n            this.$request.put('/user/update',this.user).then(res=>{\r\n                if(res.code==0){\r\n                    this.$message.success('修改成功')\r\n                    localStorage.setItem('user',JSON.stringify(this.user))\r\n                }else{\r\n                    this.$message.error(res.msg)\r\n                }\r\n            })\r\n            \r\n        }\r\n\r\n    }\r\n }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n ::v-deep .el-form-item__label{\r\n    font-weight: bold;\r\n }\r\n \r\n</style>\r\n"],"mappings":"AAsBA;EACAA,IAAA;EACAC,KAAA;IAEA,IAAAC,YAAA,GAAAA,CAAAC,IAAA,EAAAC,KAAA,EAAAC,QAAA;MACA,IAAAD,KAAA;QACA,OAAAC,QAAA,KAAAC,KAAA;MACA;QACA,SAAAC,QAAA,CAAAC,aAAA;UACA,KAAAC,KAAA,CAAAF,QAAA,CAAAG,aAAA;QACA;QACA,OAAAL,QAAA;MACA;IACA;IACA,IAAAM,aAAA,GAAAA,CAAAR,IAAA,EAAAC,KAAA,EAAAC,QAAA;MACA,IAAAD,KAAA;QACA,OAAAC,QAAA,KAAAC,KAAA;MACA,WAAAF,KAAA,UAAAG,QAAA,CAAAK,WAAA;QACA,OAAAP,QAAA,KAAAC,KAAA;MACA;QACA,OAAAD,QAAA;MACA;IACA;IAEA;MACAQ,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MAEAV,QAAA;QACAK,WAAA;QACAJ,aAAA;MACA;MACAU,KAAA;QACAN,WAAA,GACA;UAAAO,QAAA;UAAAC,SAAA,EAAAlB,YAAA;UAAAmB,OAAA;QAAA,EACA;QACAb,aAAA,GACA;UAAAW,QAAA;UAAAC,SAAA,EAAAT,aAAA;UAAAU,OAAA;QAAA;MAEA;IAIA;EACA;EACAC,UAAA,GAEA;EACAC,OAAA;IACAC,OAAA;MACA,KAAAC,QAAA,CAAAC,GAAA,sBAAAb,IAAA,EAAAc,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACAf,YAAA,CAAAgB,OAAA,SAAAlB,IAAA,CAAAmB,SAAA,MAAApB,IAAA;QACA;UACA,KAAAiB,QAAA,CAAAI,KAAA,CAAAN,GAAA,CAAAO,GAAA;QACA;MACA;IAEA;EAEA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}