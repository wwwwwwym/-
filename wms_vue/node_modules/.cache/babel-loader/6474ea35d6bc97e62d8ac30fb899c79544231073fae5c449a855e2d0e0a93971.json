 if (this.ruleForm.checkPass !== '') {\r\n            this.$refs.ruleForm.validateField('checkPass');\r\n          }\r\n          callback();\r\n        }\r\n      };\r\n\r\n       return {\r\n        comName:\"login\",\r\n        ruleForm: {\r\n          pass: '',\r\n          id: ''\r\n       },\r\n       rules: {\r\n          pass: [\r\n            { validator: validatePass, trigger: 'blur' }\r\n          ],\r\n          id: [\r\n            { validator: checkId, trigger: 'blur' }\r\n          ],\r\n        }\r\n       };\r\n    },\r\n    components: {\r\n\r\n    },\r\n    methods: { \r\n        submitForm(formName) {\r\n        this.$refs[formName].validate((valid) => {\r\n          if (valid) {\r\n            alert('submit!');\r\n          } else {\r\n            console.log('error submit!!');\r\n            return false;\r\n          }\r\n        });\r\n      },\r\n      resetForm(formName) {\r\n        this.$refs[formName].resetFields();\r\n      },\r\n      toregister(){\r\n        this.$router.push({path:'/register'})\r\n      },\r\n      tomain(){\r\n        this.$router.push({path:'/main'})\r\n      }\r\n\r\n        \r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.demo-ruleForm{\r\n    margin: 0 auto;        /* 水平居中 */\r\n    padding: 20px;\r\n    margin-top:250px\r\n}\r\n\r\n.el-form-item {\r\n  margin-bottom: 22px; /* 默认是 18px，增加间距 */\r\n  text-align: left;\r\n}              \r\n \r\n</style>\r\n"],"mappings":";AAyBA;AACA;EACAA,IAAA;EACAC,KAAA;IACA;IACA,IAAAC,OAAA,GAAAA,CAAAC,IAAA,EAAAC,KAAA,EAAAC,QAAA;MACA,KAAAD,KAAA;QACA,OAAAC,QAAA,KAAAC,KAAA;MACA;MACAC,UAAA;QACA,KAAAC,MAAA,CAAAC,SAAA,CAAAL,KAAA;UACAC,QAAA,KAAAC,KAAA;QACA;UACA,IAAAI,MAAA;YACAL,QAAA,KAAAC,KAAA;UACA;YACAD,QAAA;UACA;QACA;MACA;IACA;;IAEA;IACA,IAAAM,YAAA,GAAAA,CAAAR,IAAA,EAAAC,KAAA,EAAAC,QAAA;MACA,IAAAD,KAAA;QACAC,QAAA,KAAAC,KAAA;MACA;QACA,SAAAM,QAAA,CAAAC,SAAA;UACA,KAAAC,KAAA,CAAAF,QAAA,CAAAG,aAAA;QACA;QACAV,QAAA;MACA;IACA;IAEA;MACAW,OAAA;MACAJ,QAAA;QACAK,IAAA;QACAC,EAAA;MACA;MAC