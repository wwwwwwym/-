{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport request from '@/request/request';\nimport dayjs from 'dayjs';\nexport default {\n  created() {\n    this.load();\n  },\n  data() {\n    return {\n      value1: '',\n      tableData: [],\n      pageNum: 1,\n      //当前页码\n      pageSize: 5,\n      //每页个数\n      username: '',\n      userid: '',\n      total: 0,\n      deposity: '',\n      FormVisible: false,\n      form: {},\n      deposityValue: '',\n      options: [{\n        value: '选项1',\n        label: '仓库1'\n      }, {\n        value: '选项2',\n        label: '仓库2'\n      }, {\n        value: '选项3',\n        label: '仓库3'\n      }, {\n        value: '选项4',\n        label: '仓库4'\n      }, {\n        value: '选项5',\n        label: '仓库5'\n      }]\n    };\n  },\n  methods: {\n    handleAdd() {\n      this.form = {};\n      this.FormVisible = true;\n    },\n    save() {},\n    reset() {\n      this.username = '';\n      this.userid = '';\n      this.deposity = '';\n      this.load(this.pageNum);\n    },\n    load(pageNum) {\n      //分页查询\n      if (pageNum) {\n        this.pageNum = pageNum;\n      }\n      request.get('/user/selectByPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          userid: this.userid,\n          username: this.username,\n          deposity: this.deposity\n        }\n      }).then(res => {\n        if (res.code == 0) {\n          this.tableData = res.data.records;\n          this.total = res.data.total;\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    formatTime(row, column, cellValue) {\n      if (!cellValue) return '暂无时间'; // 处理空值\n      return dayjs(cellValue).format('YYYY-MM-DD');\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load(pageNum);\n    },\n    handleDepositChange(value) {\n      // 根据 value 查找对应的 label\n      const selectedOption = this.options.find(item => item.value === value);\n      if (selectedOption) {\n        this.user.deposity = selectedOption.label; // 将 label 存入 ruleForm\n      }\n    }\n  }\n};","map":{"version":3,"names":["request","dayjs","created","load","data","value1","tableData","pageNum","pageSize","username","userid","total","deposity","FormVisible","form","deposityValue","options","value","label","methods","handleAdd","save","reset","get","params","then","res","code","records","$notify","error","msg","formatTime","row","column","cellValue","format","handleCurrentChange","handleDepositChange","selectedOption","find","item","user"],"sources":["src/components/manager_personnel/manager_personnel.vue"],"sourcesContent":["<template>\r\n <div>\r\n  \r\n  <div style=\"display:flex;\">\r\n<!-- 查询组件 -->\r\n\r\n      <el-input v-model=\"deposity\" placeholder=\"请输入负责仓库\" style=\"width:20%; margin-left: 10px\"></el-input>\r\n      <el-input v-model=\"username\" placeholder=\"请输入用户名\" style=\"width:20%; margin-left: 10px\"></el-input>\r\n      <el-input v-model=\"userid\" placeholder=\"请输入工号\" style=\"width:20%; margin-left: 10px\"></el-input>  \r\n\r\n      <el-button type=\"primary\" style=\"margin-left: 40px\" @click=\"load(1)\"><i class=\"el-icon-search\"></i> 查询</el-button>\r\n      <el-button type=\"primary\" style=\"margin-left: 10px\" @click=\"reset\"><i class=\"el-icon-refresh-right\"></i> 重置</el-button>\r\n  </div>\r\n  <div style=\"margin-top: 20px;margin-bottom: 10px\">\r\n    <el-button type=\"primary\" plain @click=\"handleAdd\">新增</el-button>\r\n    <el-button type=\"danger\" plain>批量删除</el-button>\r\n  </div>\r\n  \r\n\r\n<!-- 表单区域 -->\r\n  <el-table :data=\"tableData\" stripe border style=\"width: 100%\" \r\n    :cell-style=\"{textAlign:'center'}\" :header-cell-style=\"{textAlign:'center',backgroundColor:'aliceblue',color:'#666'}\">\r\n    <!-- 多选 -->\r\n    <el-table-column type=\"selection\" width=\"55\" align=\"center\"> </el-table-column>\r\n    <el-table-column prop=\"userid\" label=\"用户id\"> </el-table-column>\r\n    <el-table-column prop=\"username\" label=\"用户名称\" > </el-table-column>\r\n    <el-table-column prop=\"roleid\" label=\"角色id\" > </el-table-column>\r\n    <el-table-column prop=\"deposity\" label=\"负责仓库\" > </el-table-column>\r\n    <el-table-column prop=\"telephone\" label=\"电话号码\" > </el-table-column>\r\n    <el-table-column prop=\"time\" label=\"入职时间\" :formatter=\"formatTime\"> </el-table-column>\r\n    <el-table-column fixed=\"right\" label=\"操作\" width=\"180\" >\r\n      <template v-slot=\"scope\">\r\n        <el-button @click=\"handleClick(scope.row)\" type=\"text\" size=\"medium\">编辑</el-button>\r\n        <el-button type=\"text\" size=\"medium\">删除</el-button>\r\n      </template>\r\n    </el-table-column>\r\n  </el-table>\r\n\r\n\r\n<!-- 分页组件 -->\r\n  <div style=\"margin:10px\">\r\n    <el-pagination\r\n      @current-change=\"handleCurrentChange\"\r\n      :current-page=\"pageNum\"\r\n      :page-sizes=\"[5, 10, 15, 20]\"\r\n      :page-size=\"pageSize\"\r\n      layout=\"total, prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n\r\n\r\n<!-- 新增弹出框 -->\r\n<el-dialog title=\"收货地址\" :visible.sync=\"FormVisible\" width=\"30%\">\r\n    <el-form :model=\"form\" :rules=\"rules\" label-width=\"80px\" style=\"padding-right:20px\" ref=\"form\" >\r\n        <el-form-item label=\"用户名\" prop=\"username\">\r\n            <el-input placeholder=\"请输入用户名\" v-model=\"form.username\" ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"工号\" prop=\"userid\">\r\n            <el-input placeholder=\"请输入工号\" v-model=\"form.userid\" ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"电话\" prop=\"telephone\">\r\n            <el-input placeholder=\"请输入电话\" v-model=\"form.telephone\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"角色\" prop=\"roleid\">\r\n            <el-input placeholder=\"请输入角色\" v-model=\"form.roleid\" ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"负责仓库\">\r\n            <el-select v-model=\"deposityValue\" @change=\"handleDepositChange\" placeholder=\"请选择所负责仓库\" style=\"width:100%;\">\r\n                <el-option  v-for=\"item in options\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\" > </el-option>\r\n            </el-select>\r\n        </el-form-item>\r\n    </el-form>\r\n    <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n    </div>\r\n</el-dialog>\r\n\r\n\r\n </div>\r\n</template>\r\n\r\n<script>\r\nimport request from '@/request/request'\r\nimport dayjs from 'dayjs'\r\nexport default {\r\n  created(){\r\n    this.load()\r\n  },\r\n    data() {\r\n      return {\r\n        value1: '',\r\n        tableData: [],\r\n        pageNum: 1,//当前页码\r\n        pageSize: 5,//每页个数\r\n        username: '',\r\n        userid: '',\r\n        total: 0,\r\n        deposity: '',\r\n        FormVisible:false,\r\n\r\n        form:{},\r\n        deposityValue:'',\r\n   \r\n        options: [{\r\n          value: '选项1',\r\n          label: '仓库1'\r\n        }, {\r\n          value: '选项2',\r\n          label: '仓库2'\r\n        }, {\r\n          value: '选项3',\r\n          label: '仓库3'\r\n        }, {\r\n          value: '选项4',\r\n          label: '仓库4'\r\n        }, {\r\n          value: '选项5',\r\n          label: '仓库5'\r\n        }],\r\n\r\n\r\n      }\r\n    },\r\n    methods: {\r\n      handleAdd(){\r\n        this.form={}\r\n        this.FormVisible=true\r\n\r\n      },\r\n      save(){\r\n\r\n      },\r\n      reset(){\r\n        this.username=''\r\n        this.userid=''\r\n        this.deposity=''\r\n        this.load(this.pageNum)\r\n      },\r\n      load(pageNum){//分页查询\r\n      if(pageNum)\r\n      {\r\n        this.pageNum=pageNum\r\n      }\r\n        request.get('/user/selectByPage', {\r\n          params: {\r\n            pageNum: this.pageNum,\r\n            pageSize: this.pageSize,\r\n            userid: this.userid,\r\n            username: this.username,\r\n            deposity:this.deposity\r\n          }\r\n        }).then(res => {\r\n          if(res.code == 0){\r\n            this.tableData = res.data.records\r\n            this.total=res.data.total\r\n          }\r\n          else{\r\n            this.$notify.error(res.msg);\r\n          }\r\n    \r\n        })\r\n      },\r\n      formatTime(row, column, cellValue) {\r\n      if (!cellValue) return '暂无时间'; // 处理空值\r\n      return dayjs(cellValue).format('YYYY-MM-DD');\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      this.pageNum=pageNum\r\n      this.load(pageNum)\r\n    },\r\n    handleDepositChange(value) {\r\n      // 根据 value 查找对应的 label\r\n      const selectedOption = this.options.find(item => item.value === value);\r\n      if (selectedOption) {\r\n        this.user.deposity = selectedOption.label; // 将 label 存入 ruleForm\r\n      }\r\n    }\r\n\r\n    },\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n.title{\r\n  text-align:left;\r\n}\r\n \r\n</style>\r\n"],"mappings":";;AAoFA,OAAAA,OAAA;AACA,OAAAC,KAAA;AACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,KAAA;IACA;MACAC,MAAA;MACAC,SAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,QAAA;MACAC,MAAA;MACAC,KAAA;MACAC,QAAA;MACAC,WAAA;MAEAC,IAAA;MACAC,aAAA;MAEAC,OAAA;QACAC,KAAA;QACAC,KAAA;MACA;QACAD,KAAA;QACAC,KAAA;MACA;QACAD,KAAA;QACAC,KAAA;MACA;QACAD,KAAA;QACAC,KAAA;MACA;QACAD,KAAA;QACAC,KAAA;MACA;IAGA;EACA;EACAC,OAAA;IACAC,UAAA;MACA,KAAAN,IAAA;MACA,KAAAD,WAAA;IAEA;IACAQ,KAAA,GAEA;IACAC,MAAA;MACA,KAAAb,QAAA;MACA,KAAAC,MAAA;MACA,KAAAE,QAAA;MACA,KAAAT,IAAA,MAAAI,OAAA;IACA;IACAJ,KAAAI,OAAA;MAAA;MACA,IAAAA,OAAA,EACA;QACA,KAAAA,OAAA,GAAAA,OAAA;MACA;MACAP,OAAA,CAAAuB,GAAA;QACAC,MAAA;UACAjB,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAE,MAAA,OAAAA,MAAA;UACAD,QAAA,OAAAA,QAAA;UACAG,QAAA,OAAAA;QACA;MACA,GAAAa,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAA