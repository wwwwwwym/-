{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport request from '@/request/request';\nexport default {\n  created() {\n    this.load();\n  },\n  data() {\n    return {\n      tableData: [],\n      pageNum: 1,\n      //当前页码\n      pageSize: 10,\n      //每页个数\n      pname: '',\n      recordId: '',\n      total: 0,\n      record_ids: [],\n      user: JSON.parse(localStorage.getItem('user') || '{}'),\n      deposityNew: '',\n      deposityOld: '',\n      applyTime: '',\n      type: '',\n      detailFormVisible: false,\n      form: {\n        recordId: '',\n        pname: '',\n        type: '',\n        deposityNew: '',\n        deposityOld: '',\n        quantity: '',\n        price: '',\n        state: '',\n        applyId: '',\n        applyTime: '',\n        applyRemark: '',\n        reviewId: '',\n        reviewTime: '',\n        reviewRemark: ''\n      }\n    };\n  },\n  methods: {\n    handleDetail(row) {\n      this.form = JSON.parse(JSON.stringify(row)); //给form对象赋值，深拷贝\n      this.detailFormVisible = true;\n    },\n    handleAvatarSuccess(response) {\n      this.form.picture = response.data;\n      console.log(response.data);\n    },\n    reset() {\n      this.pname = '';\n      this.deposityNew = '';\n      this.deposityOld = '';\n      this.applyTime = '';\n      this.type = '';\n      this.load(this.pageNum);\n    },\n    handleImport(res) {\n      if (res.code === 0) {\n        this.$message.success(\"导入成功\");\n        this.load(1);\n      } else {\n        this.$message.error(res.msg);\n      }\n    },\n    exportData() {\n      //批量导出\n      console.log(this.record_ids.length);\n      if (!this.record_ids.length) {\n        //没有选择行的时候，全部导出，或者根据搜索条件查询到的数据全部导出\n        window.open('http://localhost:9000/recordIn/export?token=' + this.user.token + \"&pname=\" + this.pname + \"&deposity=\" + this.deposity);\n      } else {\n        let record_idsStr = this.record_ids.join(',');\n        window.open('http://localhost:9000/recordIn/export?token=' + this.user.token + \"&record_ids=\" + record_idsStr); //传了选中的行\n      }\n    },\n    handleSelectionChange(rows) {\n      console.log(\"原始 rows 数据:\", rows);\n      this.record_ids = rows.map(v => v.recordId); //把对象数组变成数字数组,v.recordId与前端获取的row的字段名一致，刚开始写recoord_id一直不成功\n      console.log(this.record_ids[0]);\n    },\n    load(pageNum) {\n      //分页查询\n      if (pageNum) {\n        this.pageNum = pageNum;\n      }\n      request.get('/recordIn/selectByPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          pname: this.pname,\n          deposityNew: this.deposityNew,\n          deposityOld: this.deposityOld,\n          type: this.type,\n          applyTime: this.applyTime\n        }\n      }).then(res => {\n        if (res.code == 0) {\n          this.tableData = res.data.records;\n          this.total = res.data.total;\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["request","created","load","data","tableData","pageNum","pageSize","pname","recordId","total","record_ids","user","JSON","parse","localStorage","getItem","deposityNew","deposityOld","applyTime","type","detailFormVisible","form","quantity","price","state","applyId","applyRemark","reviewId","reviewTime","reviewRemark","methods","handleDetail","row","stringify","handleAvatarSuccess","response","picture","console","log","reset","handleImport","res","code","$message","success","error","msg","exportData","length","window","open","token","deposity","record_idsStr","join","handleSelectionChange","rows","map","v","get","params","then","records","$notify","handleCurrentChange"],"sources":["src/components/manager_record/search_record.vue"],"sourcesContent":["<template>\r\n <div>\r\n  \r\n  <div style=\"margin-top:20px; margin-bottom: 30px; display:flex;\">\r\n<!-- 查询组件 -->\r\n      <!-- <el-select style=\"margin-left: 10px;\" v-model=\"value\" filterable placeholder=\"请选择来源仓库\">\r\n        <el-option\r\n          v-for=\"item in options\"\r\n          :key=\"item.value\"\r\n          :label=\"item.label\"\r\n          :value=\"item.value\">\r\n        </el-option>\r\n      </el-select> -->\r\n      <!-- <el-input v-model=\"deposity\" placeholder=\"请输入来源仓库\" style=\"width:20%; margin-left: 10px\"></el-input> -->\r\n      <el-select v-model=\"form.deposityNew\" placeholder=\"请选择新仓库\" style=\"width:20%; margin-left: 10px\">\r\n        <el-option v-for=\"item in ['仓库1','仓库2','仓库3','仓库4','仓库5']\" :key=\"item\" :label=\"item\" :value=\"item\"></el-option>\r\n      </el-select>\r\n      <el-select v-model=\"form.deposityOld\" placeholder=\"请选择原仓库\" style=\"width:20%; margin-left: 10px\">\r\n        <el-option v-for=\"item in ['仓库1','仓库2','仓库3','仓库4','仓库5']\" :key=\"item\" :label=\"item\" :value=\"item\"></el-option>\r\n      </el-select>\r\n      <el-select v-model=\"form.type\" placeholder=\"请选择记录类型\" style=\"width:20%; margin-left: 10px\">\r\n        <el-option v-for=\"item in ['出货单','进货单','调货单']\" :key=\"item\" :label=\"item\" :value=\"item\"></el-option>\r\n      </el-select>\r\n      \r\n      <div class=\"block\" style=\"margin-left: 10px\">\r\n        <el-date-picker\r\n          v-model=\"applyTime\"\r\n          type=\"date\"\r\n          placeholder=\"选择日期\"\r\n          value-format=\"yyyy-MM-dd\">\r\n        </el-date-picker>\r\n      </div>\r\n\r\n      <el-input v-model=\"form.pname\" placeholder=\"请输入产品名称\" style=\"width:20%; margin-left: 10px\"></el-input> \r\n\r\n      <el-button type=\"primary\" style=\"margin-left: 40px\" @click=\"load(1)\"><i class=\"el-icon-search\"></i> 查询</el-button>\r\n      <el-button type=\"primary\" style=\"margin-left: 10px\" @click=\"reset\"><i class=\"el-icon-refresh-right\"></i> 重置</el-button>\r\n  </div>\r\n\r\n\r\n  <div style=\"margin-top: 20px;margin-bottom: 10px\">\r\n    <!-- <el-button type=\"primary\" plain @click=\"handleAdd\">新增</el-button>\r\n    <el-button type=\"danger\" plain @click=\"delBatch\">批量删除</el-button> -->\r\n    <el-button type=\"primary\" plain @click=\"exportData\">批量导出</el-button>\r\n    <el-upload action=\"http://localhost:9000/recordIn/import\" :headers=\"{token:user.token}\" :on-success=\"handleImport\" style=\"display:inline-block ;margin-left: 10px\" :show-file-list=\"false\">\r\n      <el-button type=\"primary\" plain>批量导入</el-button>\r\n    </el-upload>\r\n  </div>\r\n  \r\n\r\n<!-- 表单区域 -->\r\n  <el-table :data=\"tableData\" stripe border style=\"width: 100%\" \r\n    :cell-style=\"{textAlign:'center'}\" :header-cell-style=\"{textAlign:'center',backgroundColor:'aliceblue',color:'#666'}\"\r\n    @selection-change=\"handleSelectionChange\">\r\n    <!-- 多选 -->\r\n    <el-table-column type=\"selection\" width=\"55\" align=\"center\"> </el-table-column>\r\n    <el-table-column prop=\"recordId\" label=\"记录编号\" width=\"90\"> </el-table-column>\r\n    <el-table-column prop=\"pname\" label=\"产品名称\" > </el-table-column>\r\n    <el-table-column prop=\"type\" label=\"记录类型\" > </el-table-column>\r\n    <el-table-column prop=\"deposityNew\" label=\"新仓库\" width=\"90\"> </el-table-column>\r\n    <el-table-column prop=\"deposityOld\" label=\"原仓库\" width=\"90\"> </el-table-column>\r\n    <el-table-column prop=\"state\" label=\"当前状态\" width=\"90\"> </el-table-column>\r\n    <el-table-column prop=\"applyId\" label=\"申请人\" width=\"90\"> </el-table-column>\r\n    <el-table-column prop=\"applyTime\" label=\"申请时间\" width=\"180\" > </el-table-column>\r\n    <el-table-column prop=\"reviewId\" label=\"审核人\" width=\"90\"> </el-table-column>\r\n    <el-table-column prop=\"reviewTime\" label=\"审核时间\" width=\"180\"> </el-table-column>\r\n    <el-table-column fixed=\"right\" label=\"操作\" >\r\n      <template slot-scope=\"scope\">\r\n        <el-button  type=\"text\" size=\"medium\" @click=\"handleDetail(scope.row)\">查看详情</el-button>\r\n        <!-- <el-button type=\"text\" size=\"medium\" @click=\"del(scope.row.userid)\">删除</el-button> -->\r\n      </template>\r\n    </el-table-column>\r\n  </el-table>\r\n\r\n\r\n<!-- 分页组件 -->\r\n  <div style=\"margin:10px\">\r\n    <el-pagination\r\n      @current-change=\"handleCurrentChange\"\r\n      :current-page=\"pageNum\"\r\n      :page-sizes=\"[5, 10, 15, 20]\"\r\n      :page-size=\"pageSize\"\r\n      layout=\"total, prev, pager, next\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n\r\n<!-- 详情弹出框 -->\r\n<el-dialog title=\"详细信息\" :visible.sync=\"detailFormVisible\" width=\"40%\">\r\n    <el-form :model=\"form\" :rules=\"rules\" label-width=\"80px\" style=\"padding-right:20px\" ref=\"form\" >\r\n      <div style=\"margin:25px ; text-align:center;justify-content:center\" >\r\n          <img v-if=\"form.picture\" :src=\"form.picture\" class=\"avatar\">\r\n          <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n      </div>\r\n        <el-form-item label=\"记录编号\" >\r\n            <el-input  v-model=\"form.recordId\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"产品名称\" >\r\n            <el-input v-model=\"form.pname\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"记录类型\" >\r\n            <el-input v-model=\"form.type\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"原仓库\" >\r\n            <el-input v-model=\"form.deposityOld\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"新仓库\" >\r\n            <el-input v-model=\"form.deposityNew\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"数量\" >\r\n            <el-input v-model=\"form.quantity\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"价格\" >\r\n            <el-input v-model=\"form.price\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"状态\" >\r\n            <el-input v-model=\"form.state\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"申请人\" >\r\n            <el-input v-model=\"form.applyId\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"申请时间\" >\r\n            <el-input v-model=\"form.applyTime\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"申请备注\" >\r\n            <el-input v-model=\"form.applyRemark\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"审核人\" >\r\n            <el-input v-model=\"form.reviewId\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"审核时间\" >\r\n            <el-input v-model=\"form.reviewTime\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"审核意见\" >\r\n            <el-input v-model=\"form.reviewRemark\" disabled></el-input>\r\n        </el-form-item>\r\n    </el-form>\r\n    <!-- <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"editFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"update\">确 定</el-button>\r\n    </div> -->\r\n</el-dialog>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n </div>\r\n</template>\r\n\r\n<script>\r\nimport request from '@/request/request'\r\n\r\n export default {\r\n  created(){\r\n    this.load();\r\n  },\r\n    data() {\r\n      return {\r\n        tableData: [],\r\n        pageNum: 1,//当前页码\r\n        pageSize: 10,//每页个数\r\n        pname: '',\r\n        recordId: '',\r\n        total: 0,\r\n        record_ids: [],\r\n        user:JSON.parse(localStorage.getItem('user') || '{}'),\r\n        deposityNew: '',\r\n        deposityOld: '',\r\n        applyTime: '',\r\n        type: '',\r\n        detailFormVisible: false,\r\n        form:{\r\n          recordId: '',\r\n          pname: '',\r\n          type: '',\r\n          deposityNew: '',\r\n          deposityOld: '',\r\n          quantity: '',\r\n          price: '',\r\n          state: '',\r\n          applyId: '',\r\n          applyTime: '',\r\n          applyRemark: '',\r\n          reviewId: '',\r\n          reviewTime: '',\r\n          reviewRemark: '',\r\n\r\n        },\r\n\r\n      }\r\n    },\r\n    methods: {\r\n      handleDetail(row){\r\n        this.form=JSON.parse(JSON.stringify(row))//给form对象赋值，深拷贝\r\n        this.detailFormVisible=true\r\n      },\r\n      handleAvatarSuccess(response){\r\n        this.form.picture = response.data\r\n        console.log(response.data)\r\n      },\r\n      reset(){\r\n        this.pname=''\r\n        this.deposityNew=''\r\n        this.deposityOld=''\r\n        this.applyTime=''\r\n        this.type=''\r\n        this.load(this.pageNum)\r\n      },\r\n      handleImport(res){\r\n        if(res.code===0){\r\n          this.$message.success(\"导入成功\");\r\n          this.load(1)\r\n        }else{\r\n          this.$message.error(res.msg);\r\n        }\r\n      },\r\n      exportData(){//批量导出\r\n      console.log(this.record_ids.length)\r\n      if(!this.record_ids.length){//没有选择行的时候，全部导出，或者根据搜索条件查询到的数据全部导出\r\n        window.open('http://localhost:9000/recordIn/export?token='+ this.user.token + \"&pname=\"+this.pname + \"&deposity=\"+this.deposity )\r\n      }else{\r\n        let record_idsStr = this.record_ids.join(',')\r\n        window.open('http://localhost:9000/recordIn/export?token='+ this.user.token + \"&record_ids=\"+record_idsStr)//传了选中的行\r\n      }\r\n      },\r\n      handleSelectionChange(rows){   \r\n        console.log(\"原始 rows 数据:\", rows);\r\n        this.record_ids=rows.map(v=>v.recordId)//把对象数组变成数字数组,v.recordId与前端获取的row的字段名一致，刚开始写recoord_id一直不成功\r\n        console.log(this.record_ids[0])\r\n      },\r\n      load(pageNum){//分页查询\r\n      if(pageNum)\r\n      {\r\n        this.pageNum=pageNum\r\n      }\r\n        request.get('/recordIn/selectByPage', {\r\n          params: {\r\n            pageNum: this.pageNum,\r\n            pageSize: this.pageSize,\r\n            pname: this.pname,\r\n            deposityNew:this.deposityNew,\r\n            deposityOld:this.deposityOld,\r\n            type:this.type,\r\n            applyTime:this.applyTime\r\n          }\r\n        }).then(res => {\r\n          if(res.code == 0){\r\n            this.tableData = res.data.records\r\n            this.total=res.data.total\r\n          }\r\n          else{\r\n            this.$notify.error(res.msg);\r\n          }\r\n    \r\n        })\r\n      },\r\n    handleCurrentChange(pageNum){\r\n      this.pageNum=pageNum\r\n      this.load(pageNum)\r\n    },\r\n\r\n\r\n\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n.title{\r\n  text-align:left;\r\n}\r\n ::v-deep .avatar-uploader .el-upload {\r\n    border: 1px dashed #d9d9d9;\r\n    border-radius: 6px;\r\n    cursor: pointer;\r\n    position: relative;\r\n    overflow: hidden;\r\n  }\r\n   ::v-deep .avatar-uploader .el-upload:hover {\r\n    border-color: #409EFF;\r\n  }\r\n   ::v-deep .avatar-uploader-icon {\r\n    font-size: 28px;\r\n    color: #8c939d;\r\n    width: 178px;\r\n    height: 178px;\r\n    line-height: 178px;\r\n    text-align: center;\r\n  }\r\n  \r\n   ::v-deep .avatar {\r\n    width: 178px;\r\n    height: 178px;\r\n    display: block;\r\n  }\r\n \r\n</style>\r\n"],"mappings":";;AAyJA,OAAAA,OAAA;AAEA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,QAAA;MACAC,KAAA;MACAC,UAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,WAAA;MACAC,WAAA;MACAC,SAAA;MACAC,IAAA;MACAC,iBAAA;MACAC,IAAA;QACAb,QAAA;QACAD,KAAA;QACAY,IAAA;QACAH,WAAA;QACAC,WAAA;QACAK,QAAA;QACAC,KAAA;QACAC,KAAA;QACAC,OAAA;QACAP,SAAA;QACAQ,WAAA;QACAC,QAAA;QACAC,UAAA;QACAC,YAAA;MAEA;IAEA;EACA;EACAC,OAAA;IACAC,aAAAC,GAAA;MACA,KAAAX,IAAA,GAAAT,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAqB,SAAA,CAAAD,GAAA;MACA,KAAAZ,iBAAA;IACA;IACAc,oBAAAC,QAAA;MACA,KAAAd,IAAA,CAAAe,OAAA,GAAAD,QAAA,CAAAhC,IAAA;MACAkC,OAAA,CAAAC,GAAA,CAAAH,QAA