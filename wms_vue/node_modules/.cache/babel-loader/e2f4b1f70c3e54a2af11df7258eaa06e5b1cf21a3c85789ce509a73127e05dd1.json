isabled></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"入职时间\" prop=\"time\">\r\n                <el-input  v-model=\"user.time\" disabled></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"负责仓库\" prop=\"deposity\">\r\n                <el-input placeholder=\"请输入仓库名\" v-model=\"user.deposity\" ></el-input>\r\n            </el-form-item>\r\n            <div>\r\n                <el-button type=\"primary\" @click=\"update\">保 存</el-button>\r\n            </div>\r\n\r\n\r\n        </el-form>\r\n\r\n    </el-card>\r\n\r\n </div>\r\n</template>\r\nimport dayjs from 'dayjs'\r\n<script>\r\n export default {\r\n    name:'app',\r\n    data () {\r\n       return {\r\n        user:JSON.parse(localStorage.getItem('user') || '{}'),\r\n\r\n\r\n       }\r\n    },\r\n    components: {\r\n\r\n    },\r\n    methods: { \r\n        update(){\r\n            this.$request.put('/user/update',this.user).then(res=>{\r\n                if(res.code==0){\r\n                    this.$message.success('修改成功')\r\n                    localStorage.setItem('user',JSON.stringify(this.user))\r\n                }else{\r\n                    this.$message.error(res.msg)\r\n                }\r\n            })\r\n            \r\n        }\r\n\r\n    },\r\n    computed: {\r\n    // 格式化时间\r\n    formattedTime() {\r\n      return this.user.time \r\n        ? dayjs(this.user.time).format('YYYY-MM-DD') \r\n        : '';\r\n    }\r\n  }\r\n }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n\r\n ::v-deep .el-form-item__label{\r\n    font-weight: bold;\r\n }\r\n\r\n</style>\r\n"],"mappings":"AAsCA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IAGA;EACA;EACAC,UAAA,GAEA;EACAC,OAAA;IACAC,OAAA;MACA,KAAAC,QAAA,CAAAC,GAAA,sBAAAT,IAAA,EAAAU,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACAX,YAAA,CAAAY,OAAA,SAAAd,IAAA,CAAAe,SAAA,MAAAhB,IAAA;QACA;UACA,KAAAa,QAAA,CAAAI,KAAA,CAAAN,GAAA,CAAAO,GAAA;QACA;MACA;IAEA;E